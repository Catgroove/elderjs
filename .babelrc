{
  "presets": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true,
        "useESModules": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["> 0.25%", "not dead", "IE 11"]
        },
        "useBuiltIns": "usage",
        "forceAllTransforms": true,
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "modules": false
      }
    ]
  ]
}
